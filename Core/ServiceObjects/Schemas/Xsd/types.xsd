        2.  PropertyTag

      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="t:BasePathToElementType">
        <xs:attribute name="DistinguishedPropertySetId" type="t:DistinguishedPropertySetType" use="optional"/>
        <xs:attribute name="PropertySetId" type="t:GuidType" use="optional"/>
