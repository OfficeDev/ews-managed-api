      <xs:maxInclusive value="1440" />
    </xs:restriction>
  </xs:simpleType>

  <xs:complexType name="BaseSubscriptionRequestType" abstract="true">
    <xs:sequence>
      <xs:element name="FolderIds" type="t:NonEmptyArrayOfBaseFolderIdsType" minOccurs="0"/>
